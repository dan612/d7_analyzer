{
    "name": "dan612/d7_analyzer",
    "description": "D7 Site Analyzer",
    "type": "project",
    "autoload": {
        "psr-4": {
            "D7_analyzer\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Dan Chadwick",
            "email": "dan.chadwick@acquia.com"
        }
    ],
    "require": {
        "symfony/yaml": "^6",
        "guzzlehttp/guzzle": "^7.0",
        "twig/twig": "^3.7",
        "symfony/asset": "^6",
        "symfony/process": "^6",
        "symfony/filesystem": "^6",
        "symfony/console": "^6"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
        "squizlabs/php_codesniffer": "3.*",
        "phpcompatibility/php-compatibility": "*",
        "drupal/coder": "^8.3"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
